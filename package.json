{
    "name": "information-security-policy",
    "author": {
        "name": "Lullabot",
        "email": "security@lullabot.com"
    },
    "config": {
        "gitbook": "3.2.3"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:Lullabot/information-security-policy.git"
    },
    "license": "SEE LICENSE IN LICENSE",
    "scripts": {
        "install": "cross-var gitbook install --gitbook=$npm_package_config_gitbook",
        "build": "cross-var gitbook build --gitbook=$npm_package_config_gitbook",
        "pdf": "cross-var gitbook pdf --gitbook=$npm_package_config_gitbook ./",
        "serve": "cross-var gitbook serve --gitbook=$npm_package_config_gitbook",
        "test": "blcl -re --exclude /security.pdf ./_book",
        "spellcheck": "mdspell --en-us --ignore-numbers --ignore-acronyms '**/*.md' '!**/node_modules/**/*.md'"
    },
    "dependencies": {
        "cross-var": "^1.1.0",
        "highlight.js": "^9.15.8",
        "html-escape": "^2.0.0"
    },
    "devDependencies": {
        "broken-link-checker-local": "^0.2.0",
        "markdown-spellcheck": "^1.3.1"
    }
}
